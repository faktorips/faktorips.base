package org.faktorips.devtools.stdbuilder.xtend.table.templateimport org.faktorips.devtools.stdbuilder.xmodel.table.XColumnimport static org.faktorips.devtools.stdbuilder.xtend.template.MethodNames.*import org.faktorips.values.DefaultInternationalStringclass ColumnAttributeExtensionTmpl {    def package static xmlValueType(        XColumn it) {        if(multilingual) DefaultInternationalString else "String"    }    def package static addToElement(        XColumn it,        String valueParam,        String elementParam,        String tagNameParam) {        if(multilingual) addInternationalStringToElement(            valueParam,            elementParam,            tagNameParam) else addValueToElement(            valueParam,            elementParam,            tagNameParam)    }}