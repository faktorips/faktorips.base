{
  "all-versions": [
    {
      "extension": "revapi.filter",
      "configuration": {
        "elements": {
          "exclude": [
            "(missing-class|class|interface|enum|field|method .*) org\\.faktorips\\.runtime\\..*",
            "(missing-class|class|interface|enum|field|method .*) org\\.faktorips\\.valueset\\..*",
            "(missing-class|class|interface|enum|field|method .*) org\\.hamcrest\\..*"
          ]
        }
      }
    },
    {
      "extension": "revapi.differences",
      "id": "all-versions",
      "configuration": {
        "ignore": true,
        "differences": [
          {
            "ignore": true,
            "code": "java.annotation.added",
            "annotation": "@java.lang.Override",
            "justification": "false positive"
          }
        ]
      }
    }
  ],
  "21.12.0": [
    {
      "extension": "revapi.differences",
      "configuration": {
        "ignore": true,
        "differences": [       
          {
            "code": "java.class.externalClassExposedInAPI",
            "new": "interface org.faktorips.valueset.ValueSet<T>",
            "justification": "FIPS-8175"
          }      
        ]
      }
    }
  ]
}