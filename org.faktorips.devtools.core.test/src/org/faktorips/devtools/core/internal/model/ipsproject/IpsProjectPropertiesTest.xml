<?xml version="1.0" encoding="UTF-8"?>

<IpsProject modelProject="true" 
            productDefinitionProject="true" 
            javaSrcLanguage="it" 
            runtimeIdPrefix="testPrefix" 
            javaProjectContainsClassesForDynamicDatatypes="true"
            containerRelationIsImplementedRuleEnabled="true" changesInTimeNamingConvention="myConvention"> 
    <RequiredIpsFeatures>
        <RequiredIpsFeature id="required.id" minVersion="min.Version"/>
        <RequiredIpsFeature id="another.id" minVersion="min.Version.2"/>
    </RequiredIpsFeatures>
	<IpsArtefactBuilderSet id="org.faktorips.devtools.stdbuilder.ipsstdbuilderset">	
	    <IpsArtefactBuilderSetConfig>
	        <Property name="prop" value="propValue"/>
	    </IpsArtefactBuilderSetConfig>
	</IpsArtefactBuilderSet>
	<GeneratedSourcecode/>
	<ProductCmptNamingStrategy id="org.faktorips.devtools.core.DateBasedProductCmptNamingStrategy">
		<DateBasedProductCmptNamingStrategy versionIdSeparator=" " dateFormatPattern="yyyy-MM" postfixAllowed="true"/>
	</ProductCmptNamingStrategy>
	<IpsObjectPath outputDefinedPerSrcFolder="true" outputFolder="" basePackage="">
		<Entry type="src" sourceFolder="ips-src" 
			outputFolderGenerated="java-gen" basePackageGenerated="org.faktorips.sample.model"
			outputFolderExtension="java-ext" basePackageExtension="org.faktorips.sample.model"/>
	</IpsObjectPath>
	<Datatypes>
		<UsedPredefinedDatatypes>
			<Datatype id="Boolean"/>
			<Datatype id="Integer"/>
		</UsedPredefinedDatatypes>
		<DatatypeDefinitions>
			<Datatype id="PaymentMode" 
				valueObject="true"
				javaClass="org.faktorips.sample.model.PaymentMode" 
		        valueOfMethod="getPaymentMode"
		        isParsableMethod="isPaymentMode"
		        valueToStringMethod="getId"
		        specialNullValue="n"
		       	isEnumType="true"
		        getAllValuesMethod="getAllValues"/>
			<Datatype id="PaymentMode" 
				valueObject="true"
				javaClass="org.faktorips.sample.model.PaymentMode" 
		        valueOfMethod="getPaymentMode"
		        isParsableMethod="isPaymentMode"
		        valueToStringMethod="getId"
		        specialNullValue="n"
		       	isEnumType="false"
		        getAllValuesMethod=""/>
		        <Datatype id="MessageList" valueObject="false" javaClass="org.MessageList"/>
		</DatatypeDefinitions>
	</Datatypes>
	<ResourcesExcludedFromProductDefinition>
		<Resource path="src"/>
		<Resource path="build/build.xml"/>
	</ResourcesExcludedFromProductDefinition>
	<SupportedLanguages>
		<SupportedLanguage locale="en" defaultLanguage="true"/>
		<SupportedLanguage locale="de"/>
	</SupportedLanguages>
	<productRelease releaseExtensionId="myReleaseExtension" />
	<Version version="1.2.3" versionProvider="myVersionProvider" />
  <PersistenceOptions allowLazyFetchForSingleValuedAssociations="true" maxColumnNameLength="60" maxTableNameLength="30" maxTableColumnScale="10" maxTableColumnPrecision="11" maxTableColumnSize="4092">
    <TableNamingStrategy id="org.faktorips.devtools.core.CamelCaseToUpperUnderscoreTableNamingStrategy"/>
    <TableColumnNamingStrategy id="org.faktorips.devtools.core.CamelCaseToUpperUnderscoreColumnNamingStrategy"/>
  </PersistenceOptions>
  <AdditionalSettings>
  	<Setting name="derivedUnionIsImplemented"/>
  	<Setting enabled="false" name="derivedUnionIsImplemented"/>
  	<Setting name="formulaLanguageLocale" value="ko"/>
	<Setting enabled="false" name="markerEnums" value="marker; marker2"/>
	<Setting enabled="true" name="businessFunctionsForValidationRules"/>
	<Setting enabled="false" name="changingOverTimeDefault"/>
	<Setting name="duplicateProductComponentSeverity" value="ERROR"/>
  <Setting name="persistenceColumnSizeChecksSeverity" value="ERROR"/>
  <Setting name="tableContentFormat" value="CSV"/>
  </AdditionalSettings>
</IpsProject>
